{
    "name": "itbeauftragter/yii2-bootstrap5",
    "description": "NSL: The Twitter Bootstrap v5 extension for the Yii framework",
    "keywords": [
        "yii2",
        "bootstrap",
        "bootstrap5"
    ],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/itbeauftragter/yii2-bootstrap5"
    },
    "authors": [
        {
            "name": "Daniel Lucas",
            "email": "daniel.lucas@neusser-schuetzenlust.de",
            "homepage": "https://www.neusser-schuetzenlust.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "yiisoft/yii2": "^2.0.42",
        "bower-asset/bootstrap": "^5.1.0",
        "npm-asset/jquery": "^2.2"
    },
    "require-dev": {
        "yiisoft/yii2-coding-standards": "~2.0",
        "phpunit/phpunit": "^6.5.14"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "itbeauftragter\\bootstrap5\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yiiunit\\extensions\\bootstrap5\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
